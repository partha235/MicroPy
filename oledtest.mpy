from machine import Pin,I2C
import ssd1306

#You can choose any other combination of I2C pins
i2c = I2C(0,scl=Pin(1), sda=Pin(0)) # for pi pico 

oled_width = 128
oled_height = 64
oled = ssd1306.SSD1306_I2C(oled_width, oled_height, i2c)

print("hi")
oled.fill(0)
oled.text('Saga', 0, 0,200)


oled.show()